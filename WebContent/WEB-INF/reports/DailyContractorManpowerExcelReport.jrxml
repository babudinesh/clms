	<queryString>
		<![CDATA[select lss.contractorname,ddi.department_name,vdi.vendor_id, ddi.department_id, group_concat(DISTINCT  lss.section_id) Line
,a.shift, Req_Operator,Req_Others,Req_Operator_tot,Shi_Operator,Shi_Others,Shi_Operator_t,b.customer_id,b.company_id,b.location_id
,b.plant_id from vendor_details_info vdi
LEFT JOIN labor_time_report lss on (vdi.vendor_id = lss.vendor_id)
INNER JOIN department_details_info ddi on (ddi.department_id = lss.department_id)
LEFT OUTER JOIN (
select l.vendor_id, l.contractorname,l.department_id,shift,
SUM((CASE WHEN l.designation ='TAILOR' THEN 1 ELSE 0 END)) as 'Shi_Operator',
SUM((CASE WHEN l.designation ='TAILOR' THEN 0 ELSE 1 END)) as 'Shi_Others',
count( l.emp) Shi_Operator_t
from labor_time_report l
where l.business_date =  $P{businessDate}
AND Allocation_status = 'Assigned'
group by l.vendor_id, l.contractorname,l.department_id, Shift

) a on vdi.vendor_id = a.vendor_id and ddi.department_id = a.department_id

LEFT OUTER JOIN (

select a.Req_Operator,a.Req_Others,a.department_id,a.customer_id,a.company_id,a.plant_id,a.location_id,(a.req_Operator + a.Req_Others) Req_Operator_tot from (select SUM((CASE WHEN jcdi.job_title ='TAILOR' THEN 1 ELSE 0 END)) as 'Req_Operator',
SUM((CASE WHEN jcdi.job_title ='TAILOR' THEN 0 ELSE 1 END)) as 'Req_Others',mrs.department_id,mrs.customer_id,mrs.company_id,mrs.plant_id,mrs.location_id
from manpower_requisition_daywise mrs
join  job_code_details_info jcdi ON (jcdi.job_code_id = mrs.job_code_id)
WHERE
(
SELECT MAX(CONCAT(DATE_FORMAT(jcd.transaction_date, '%Y%m%d'), LPAD(jcd.Job_code_sequence_id, 2, '0')))
FROM job_code_details_info jcd
WHERE  jcdi.job_code_id = jcd.job_code_id
AND jcd.transaction_date <= CURRENT_DATE()  GROUP BY job_code_id
)
group by mrs.department_id )a

) b on  ddi.department_id = b.department_id

where lss.business_date = $P{businessDate}
and b.customer_id = $P{customerId}
and b.company_id = $P{companyId}
and b.location_id = $P{locationId}
and (vdi.vendor_id = $P{vendorId} or $P{vendorId}  = '-1')
and (ddi.department_id = $P{departmentId} or $P{departmentId} = -1)
and (a.shift = $P{shift} or $P{shift} = -1)
and b.plant_id = $P{plantId}
AND Allocation_status = 'Assigned'
group by lss.contractorname,ddi.department_name,vdi.vendor_id, ddi.department_id;]]>
	</queryString>

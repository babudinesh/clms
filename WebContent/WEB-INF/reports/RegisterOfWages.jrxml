		<queryString>
			<![CDATA[select vi.vendor_name,ltr.contractorcode,cdi.company_name,cudi.customer_name
,ltr.emp, REPLACE((CONCAT(RTRIM(wd.first_name), ' ',CASE WHEN (wd.middle_name IS NULL) THEN '' ELSE CONCAT(RTRIM(wd.middle_name),' ') END, wd.last_name)),'  ',' ') as emp_name,ltr.attendance,count(ltr.attendance)as payable_days,
CASE WHEN (jcdi.job_name IS NOT NULL AND wjd.work_skill IS NOT NULL) THEN CONCAT(jcdi.job_name,', ',wjd.work_skill) ELSE COALESCE(jcdi.job_name,wjd.work_skill, '') END AS job_name ,wrdi.daily_rate_per_day,wrdi.additional_allowance,wrdi.base_rate_per_day,(wrdi.over_time_rate_per_hour*wrdi.over_time_hours) as overtime
,wrdi.additional_allowance,wrdi.total_earnings,wrdi.pf,wrdi.esi,wrdi.pt,wrdi.total_deductions,wrdi.net_salary,
cudi.customer_id,
 coalesce(ldi.location_name,'All') as location_name,
 vi.location_id
  from $P!{schemaName}.labor_time_report ltr
INNER JOIN v_worker_details wd ON (wd.worker_code = ltr.emp)
INNER JOIN customer_details_info cudi ON (cudi.customer_id = wd.customer_id)
INNER JOIN company_details_info cdi ON (cdi.company_id = wd.company_id)
INNER JOIN vendor_details_info vi ON (vi.vendor_id = wd.vendor_id)

LEFT JOIN work_job_details wjd ON (wjd.worker_id = wd.workerid and
CONCAT(DATE_FORMAT(wjd.transaction_date, '%Y%m%d'), LPAD(wjd.sequence_id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.Sequence_Id, 2, '0')))
FROM work_job_details vdi1
WHERE  wjd.worker_id = vdi1.worker_id
AND vdi1.transaction_date <= CURRENT_DATE()
))
LEFT JOIN job_code_details_info jcdi ON (jcdi.job_code_id = wjd.job_name and
CONCAT(DATE_FORMAT(jcdi.transaction_date, '%Y%m%d'), LPAD(jcdi.job_code_sequence_id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.job_code_sequence_id, 2, '0')))
FROM job_code_details_info vdi1
WHERE  jcdi.job_code_id = vdi1.job_code_id
AND vdi1.transaction_date <= CURRENT_DATE()
))
left join wage_calculator wrdi on (wrdi.worker_id = wjd.worker_id and
CONCAT(DATE_FORMAT(wrdi.transaction_date, '%Y%m%d'), LPAD(wrdi.Sequence_Id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.Sequence_Id, 2, '0')))
FROM wage_calculator vdi1
WHERE  wrdi.worker_id = vdi1.worker_id
AND vdi1.transaction_date <= CURRENT_DATE()
))
 Left JOIN location_details_info ldi on (ldi.location_id = vi.location_id and
CONCAT(DATE_FORMAT(ldi.transaction_date, '%Y%m%d'), LPAD(ldi.location_Sequence_Id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.location_Sequence_Id, 2, '0')))
FROM location_details_info vdi1
WHERE  ldi.location_id = vdi1.location_id
AND vdi1.transaction_date <= CURRENT_DATE()
))

where
CONCAT(DATE_FORMAT(vi.transaction_date, '%Y%m%d'), LPAD(vi.sequence_id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.Sequence_Id, 2, '0')))
FROM vendor_details_info vdi1
WHERE  vi.vendor_id = vdi1.vendor_id
AND vdi1.transaction_date <= CURRENT_DATE()
)

and
CONCAT(DATE_FORMAT(cdi.transaction_date, '%Y%m%d'), LPAD(cdi.company_sequence_id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.company_sequence_id, 2, '0')))
FROM company_details_info vdi1
WHERE  cdi.company_id = vdi1.company_id
AND vdi1.transaction_date <= CURRENT_DATE()
)
and
CONCAT(DATE_FORMAT(cudi.transaction_date, '%Y%m%d'), LPAD(cudi.customer_sequence_id, 2, '0')) =
(
SELECT MAX(CONCAT(DATE_FORMAT(vdi1.transaction_date, '%Y%m%d'), LPAD(vdi1.customer_sequence_id, 2, '0')))
FROM customer_details_info vdi1
WHERE  cudi.customer_id = vdi1.customer_id
AND vdi1.transaction_date <= CURRENT_DATE()
)

and ltr.attendance = 'Present'

and ( wd.vendor_id = $P{vendorId})
and  ( wd.workerid = $P{workerId} or $P{workerId} = '-1')
AND (wd.worker_code = $P{workerCode} OR $P{workerCode} = -1)
AND (ltr.emp_name LIKE '%$P!{workerName}%'  )
and wd.company_id = $P{companyId}
and wd.customer_id = $P{customerId}
and ltr.business_date between $P{fromdate} and $P{todate}
group by ltr.emp;]]>
		</queryString>
